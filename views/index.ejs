      </div>
      <br />
      <br />
      <div class="attention">
        <div class="attention1">인기 상품</div>
        <br />
        <div class="att-box">
          <div class="att-box1"></div>
          <div class="att-box2"></div>
          <div class="att-box3"></div>
          <div class="att-box4"></div>
        </div>
      </div>
    </div>
    <br />
    <br />
    <%-include("main/footer.ejs")%>
